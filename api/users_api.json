				"properties"	:	{}
			}
		},
		{
			"description"	: "Update users of a course",
			"href"	:	"/users/update",
			"title"	:	"update",
			"method"	:	"POST",
			"rel"	:	"update",
			"schema"	:	{
				"type"	:	"object",
				"required"	:	["courseID", "users"],
				"additionalProperties"	:	false,
				"properties"	:	{
					"courseID"	:	{
						"$ref"	:	"#/definitions/courseID"
					},
					"users"	:	{
						"$ref"	:	"#/definitions/users"
