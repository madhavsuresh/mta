			}
		},
		{
			"description"	: "Drop users from a course",
			"href"	:	"/users/delete",
			"title"	:	"delete",
			"method"	:	"POST",
			"rel"	:	"drop",
			"schema"	:	{
				"type"	:	"object",
				"required"	:	["courseID", "users"],
				"additionalProperties"	:	false,
				"properties"	:	{
