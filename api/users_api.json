			"targetSchema"	:	{
				"type"	: "object",
				"required"	:	[],
				"additionalProperties"	:	"false",
				"properties"	:	{}
			}
		},
		{
			"description"	: "List users of a course",
			"href"	:	"/users/get",
			"title"	:	"get",
			"method"	:	"GET",
			"rel"	:	"list",
			"schema"	:	{
				"type"	:	"object",
				"required"	:	["courseID"],
				"additionalProperties"	:	false,
				"properties"	:	{
					"courseID"	:	{
						"$ref"	:	"#/definitions/courseID"
					},
					"users"	:	{
